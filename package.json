{
  "name": "nuxt_App",
  "version": "1.0.0",
  "description": "My hunky-dory Nuxt.js project",
  "author": "guamu",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "fix": "./node_modules/.bin/eslint ./layouts/*.vue ./pages/*/*.vue ./pages/*.vue ./assets/js/*.js ./store/*.js --fix",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "element-ui": "^2.4.6",
    "express": "^4.16.3",
    "leaflet": "^1.3.4",
    "nuxt": "^2.0.0",
    "nuxt-leaflet": "0.0.10",
    "vue2-leaflet": "^1.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.7.1",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "vue-particles": "^1.0.9"
  }
}
